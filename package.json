{
  "name": "storybook-svelte-bugreproduction",
  "version": "0.0.1",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "package": "svelte-kit package",
    "package:watch": "svelte-kit package --watch",
    "preview": "vite preview --port 4000",
    "postinstall": "svelte-kit sync",
    "prepare": "husky install",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate:css-json": "postcss src/assets/styles/global.css > /dev/null",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "STORYBOOK_PROD=1 storybook build",
    "dist": "npm run package && npm publish",
    "beta": "npm run package && npm publish --tag beta"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-a11y": "^7.0.0-beta.21",
    "@storybook/addon-actions": "^7.0.0-beta.21",
    "@storybook/addon-essentials": "^7.0.0-beta.21",
    "@storybook/addon-interactions": "^7.0.0-beta.21",
    "@storybook/addon-links": "^7.0.0-beta.21",
    "@storybook/addon-svelte-csf": "^3.0.0-next.3",
    "@storybook/svelte": "^7.0.0-beta.21",
    "@storybook/sveltekit": "^7.0.0-beta.21",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "cssnano": "^5.1.14",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-custom-media": "^8.0.2",
    "postcss-custom-properties": "^12.1.11",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.21",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-loader": "^3.1.4",
    "svelte-preprocess": "^5.0.0",
    "svelte2tsx": "^0.6.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "type": "module",
  "dependencies": {
    "@storybook/addon-postcss": "^3.0.0-alpha.1",
    "@storybook/builder-vite": "^7.0.0-beta.21",
    "@storybook/client-logger": "^7.0.0-beta.21"
  }
}
